    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginSignUp</servlet-name>
        <servlet-class>controller.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminlist</servlet-name>
        <servlet-class>controller.adminlist</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginSignUp</servlet-name>
        <url-pattern>/LoginSignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminList</servlet-name>
        <url-pattern>/adminlist</url-pattern>
    </servlet-mapping>
